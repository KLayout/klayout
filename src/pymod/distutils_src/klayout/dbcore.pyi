    lm.map("1/0-255 : ONE (1/0)")
    lm.map("2/0-255 : TWO (2/0)")
    lm.map("3/0-255 : THREE (3/0)")

    # read the layout using the layer map
    lo = RBA::LoadLayoutOptions::new
    lo.layer_map.assign(lm)
    ly = RBA::Layout::new
    ly.read("input.gds", lo)
